<h1>Engagements</h1>

<table>
  <tr>
    <th>Reference</th>
    <th>Job reference</th>
    <th>Job title</th>
    <th>Provider Team</th>
    <th></th>
  </tr>

<% @engagements.each do |engagement| %>
  <tr>
    <td><%= engagement.reference %></td>
    <td><%= engagement.job__reference %></td>
    <td><%= engagement.job__title %></td>
    <td><%= engagement.provider_team__name %></td>
    <td><%= link_to 'Show', engagement.reference %></td>
  </tr>
<% end -%>
</table>

<br />
